<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<PackageId>BarelyFunctional</PackageId>
		<Version>1.0.6</Version>
		<Authors>Eivind Antonsen</Authors>
		<Description>A tiny, bare bones functional library for C#</Description>
		<RepositoryUrl>https://github.com/EivindAntonsen/BarelyFunctional</RepositoryUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/EivindAntonsen/BarelyFunctional</PackageProjectUrl>
	</PropertyGroup>

</Project>